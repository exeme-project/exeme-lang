import "std.cf"
import "std.io"

use cf::range

; Returns whether `n` itself is prime.
func isPrime(num: i32) -> bool {
	for range(2, num) => div {
		if num % div == 0 { ; Can be divided without a remainder
			return false
		}
	}

	return true
}

func main() {
	io:out(isPrime(io::in("Enter a number: ")))
}

